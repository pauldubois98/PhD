@article{AAPM-TG119,
	author = {Mynampati, Dinesh Kumar and Yaparpalvi, Ravindra and Hong, Linda and Kuo, Hsiang-Chi and Mah, Dennis},
	title = {Application of AAPM TG 119 to volumetric arc therapy (VMAT)},
	journal = {Journal of Applied Clinical Medical Physics},
	volume = {13},
	number = {5},
	pages = {108-116},
	keywords = {VMAT, IMRT, TG 119, quality assurance},
	doi = {https://doi.org/10.1120/jacmp.v13i5.3382},
	url = {https://aapm.onlinelibrary.wiley.com/doi/abs/10.1120/jacmp.v13i5.3382},
	eprint = {https://aapm.onlinelibrary.wiley.com/doi/pdf/10.1120/jacmp.v13i5.3382},
	abstract = {The purpose of this study was to create AAPM TG 119 benchmark plans for volumetric arc therapy (VMAT) and to compare VMAT plans with IMRT plan data. AAPM TG 119 proposes a set of test clinical cases for testing the accuracy of IMRT planning and delivery system. For these test cases, we generated two treatment plans, the first plan using 7–9 static dMLC IMRT fields and a second plan utilizing one- or two-arc VMAT technique. Dose optimization and calculations performed using 6 MV photons and Eclipse treatment planning system. Dose prescription and planning objectives were set according to the TG 119 goals. Plans were scored based on TG 119 planning objectives. Treatment plans were compared using conformity index (CI) for reference dose and homogeneity index (HI) (for ). F or test cases prostate, head-and-neck, C-shape and multitarget prescription dose are 75.6 Gy, 50.4 Gy, 50 Gy and 50 Gy, respectively. VMAT dose distributions were comparable to dMLC IMRT plans. Our planning results matched TG 119 planning results. For treatment plans studied, conformity indices ranged from 1.05–1.23 (IMRT) and 1.04–1.23 (VMAT). Homogeneity indices ranged from 4.6\%–11.0\% (IMRT) and 4.6\%–10.5\% (VMAT). The ratio of total monitor units necessary for dMLC IMRT to that of VMAT was in the range of 1.1–2.0. AAPM TG 119 test cases are useful to generate VMAT benchmark plans. At preclinical implementation stage, plan comparison of VMAT and IMRT plans of AAPM TG 119 test case allowed us to understand basic capabilities of VMAT technique. PACS number: 87.55.Qr},
	year = {2012}
}

