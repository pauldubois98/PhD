@article{ReportIMRT2003,
	author = {Ezzell, Gary A. and Galvin, James M. and Low, Daniel and Palta, Jatinder R. and Rosen, Isaac and Sharpe, Michael B. and Xia, Ping and Xiao, Ying and Xing, Lei and Yu, Cedric X.},
	title = {Guidance document on delivery, treatment planning, and clinical implementation of IMRT: Report of the IMRT subcommittee of the AAPM radiation therapy committee},
	journal = {Medical Physics},
	volume = {30},
	number = {8},
	pages = {2089-2115},
	keywords = {3-D conformal radiotherapy, intensity-modulated radiation therapy, inverse planning, plan optimization, quality assurance, Treatment strategy, Collimation, Wedges and compensators, Dosimetry/exposure assessment, Quality assurance equipment, intensity modulation, radiation therapy, quality control, optimisation, tumours, dosimetry, phantoms, cancer, collimators, radiation protection, shielding, Intensity modulated radiation therapy, Medical treatment planning, Radiation therapy, Dosimetry, Physicists, Drug delivery, Cancer, Radiation treatment, Medical accelerators, Linear accelerators},
	doi = {https://doi.org/10.1118/1.1591194},
	url = {https://aapm.onlinelibrary.wiley.com/doi/abs/10.1118/1.1591194},
	eprint = {https://aapm.onlinelibrary.wiley.com/doi/pdf/10.1118/1.1591194},
	abstract = {Intensity-modulated radiation therapy (IMRT) represents one of the most significant technical advances in radiation therapy since the advent of the medical linear accelerator. It allows the clinical implementation of highly conformal nonconvex dose distributions. This complex but promising treatment modality is rapidly proliferating in both academic and community practice settings. However, these advances do not come without a risk. IMRT is not just an add-on to the current radiation therapy process; it represents a new paradigm that requires the knowledge of multimodality imaging, setup uncertainties and internal organ motion, tumor control probabilities, normal tissue complication probabilities, three-dimensional (3-D) dose calculation and optimization, and dynamic beam delivery of nonuniform beam intensities. Therefore, the purpose of this report is to guide and assist the clinical medical physicist in developing and implementing a viable and safe IMRT program. The scope of the IMRT program is quite broad, encompassing multileaf-collimator-based IMRT delivery systems, goal-based inverse treatment planning, and clinical implementation of IMRT with patient-specific quality assurance. This report, while not prescribing specific procedures, provides the framework and guidance to allow clinical radiation oncology physicists to make judicious decisions in implementing a safe and efficient IMRT program in their clinics.},
	year = {2003}
}

